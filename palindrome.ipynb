   "source": [
    "n=int(input(\"enter a number:\"))\n",
    "temp=n\n",
    "reverse=0\n",
    "while temp>0:\n",
    "    r=temp%10\n",
    "    reverse=reverse*10+r\n",
    "    temp=temp//10\n",
    "if reverse==n:\n",
    "    print(\"palindrome\")\n",
    "else:\n",
    "    print(\"not palindrome\")\n",
    "    \n",
    "    \n"
   ]
